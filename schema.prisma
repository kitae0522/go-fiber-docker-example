generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
  output = "./internal/model"
  package = "model"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SchedulePost {
  id        	String   	@id @default(cuid())
  createdAt	DateTime 	@default(now())
  updatedAt	DateTime	@updatedAt
  title		String
  content	String
  author	String 
  authorID	String 		@unique
  startDate	DateTime
  endDate	DateTime
}

